<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="entity.Route" table="Route">
	 		<id name="RouteID">
	 			<generator class="native"/>
	 		</id>
	 			<property name="RouteName" length="30"/>
	 			<property name="StartPlace" length="30"/>
	 			<property name="EndPlace" length="30"/>
	 			<property name="PassageNum" length="30"/> 		
	 		 			 		
	 </class>
</hibernate-mapping>
