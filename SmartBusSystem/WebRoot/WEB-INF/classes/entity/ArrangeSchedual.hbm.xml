<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	
	<class name="entity.ArrangeSchedual" table="ArrangeSchedual">
	 		<id name="DutyRosterID">
	 			<generator class="native"/>
	 		</id>
	 		<property name="CarID" length="30"/>
	 		<property name="RouteID" length="15"/>
	 		<property name="DriverID" length="30"/>
	 		<property name="DriverName" length="12"/>
	 		<property name="Week" length="50"/>
	 		<property name="DayOfWeek" length="30"/>
	 		<property name="StartTime" length="50"/>
	 		<property name="EndTime" length="50"/>
	 		 			 		
	 </class>
</hibernate-mapping>
