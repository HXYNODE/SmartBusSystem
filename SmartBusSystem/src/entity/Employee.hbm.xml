<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

 <class name="entity.Employee" table="Employee" >
	 		<id name="EmployeeID">
	 			<generator class="native"/>
	 		</id>
	 			<property name="Name" length="30"/>
	 			<property name="Sex" length="30"/>
	 			<property name="Department" length="30"/>
	 			<property name="EGroup" length="30"/>
	 			<property name="DutyRosterID" length="30"/>	 
	 			<property name="Phone" length="30"/>	 
	 			<property name="Address" length="30"/>	 	 		
	 		 			 		
	 </class>
</hibernate-mapping>
